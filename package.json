  },
  "license": "MIT",
  "devDependencies": {
    "browserify": "^13.1.1",
    "coffee-errors": "^0.8.5",
    "coffee-script": "^1.7.1",
    "coffeeify": "^2.1.0",
    "karma": "^1.3.0",
    "karma-bro": "^0.11.1",
    "karma-chai": "^0.1.0",
    "karma-phantomjs-launcher": "^1.0.2",
    "karma-sauce-launcher": "^1.1.0",
    "karma-mocha": "^1.3.0"
  }
}
