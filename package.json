{
  "name": "iframe-script",
  "version": "1.0.1",
  "main": "./lib/iframe-script.js",
  "description": "Allows you to sandbox your client scripts inside an IFRAME to avoid conflicts when deploying to 3rd party sites.",
  "keywords": [
    "client",
    "iframe",
    "sandbox",
    "script"
  ],
  "scripts": {
    "pretest": "browserify --outfile=lib/iframe-script.js --entry=src/iframe-script.coffee --extension=.coffee --transform=coffeeify --standalone='iframe-script'",
    "test": "./test.sh",
    "dev": "./node_modules/karma/bin/karma start --no-single-run --auto-watch"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com:alexgorbatchev/iframe-script.git"
  },
  "license": "MIT",
  "devDependencies": {
    "browserify": "^4.1.6",
    "coffee-errors": "^0.8.5",
    "coffee-script": "^1.7.1",
    "coffeeify": "^0.6.0",
    "karma": "^0.12.16",
    "karma-bro": "^0.3.0",
    "karma-chai": "^0.1.0",
    "karma-phantomjs-launcher": "^0.1.4",
    "karma-sauce-launcher": "^0.2.9",
    "karma-mocha": "^0.1.3"
  }
}
